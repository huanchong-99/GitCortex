#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/e/GitCortex/.worktrees/phase-14-orchestrator-runtime/node_modules/.pnpm/lib0@0.2.117/node_modules/lib0/bin/node_modules:/proc/cygdrive/e/GitCortex/.worktrees/phase-14-orchestrator-runtime/node_modules/.pnpm/lib0@0.2.117/node_modules/lib0/node_modules:/proc/cygdrive/e/GitCortex/.worktrees/phase-14-orchestrator-runtime/node_modules/.pnpm/lib0@0.2.117/node_modules:/proc/cygdrive/e/GitCortex/.worktrees/phase-14-orchestrator-runtime/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/e/GitCortex/.worktrees/phase-14-orchestrator-runtime/node_modules/.pnpm/lib0@0.2.117/node_modules/lib0/bin/node_modules:/proc/cygdrive/e/GitCortex/.worktrees/phase-14-orchestrator-runtime/node_modules/.pnpm/lib0@0.2.117/node_modules/lib0/node_modules:/proc/cygdrive/e/GitCortex/.worktrees/phase-14-orchestrator-runtime/node_modules/.pnpm/lib0@0.2.117/node_modules:/proc/cygdrive/e/GitCortex/.worktrees/phase-14-orchestrator-runtime/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lib0/bin/gentesthtml.js" "$@"
else
  exec node  "$basedir/../lib0/bin/gentesthtml.js" "$@"
fi
