    "@tanstack/pacer-lite": "^0.1.0",
    "@tanstack/db-ivm": "0.1.13"
  },
  "devDependencies": {
    "@vitest/coverage-istanbul": "^3.2.4",
    "arktype": "^2.1.27",
    "superjson": "^2.2.5",
    "temporal-polyfill": "^0.3.0"
  },
  "exports": {
